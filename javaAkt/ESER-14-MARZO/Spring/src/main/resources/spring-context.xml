<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
       
       <bean name="employee" class="example.Employee"/>
       <bean name="fillEmployee" class="example.Employee" scope="prototype">
       		<property name="name" value="jhon"/>
       		<property name="addr" ref="address"/>
       </bean>
       
       <bean name="address" class="example.Address"/>
      
       
</beans>       