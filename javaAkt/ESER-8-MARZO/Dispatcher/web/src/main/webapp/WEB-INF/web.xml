<?xml version="1.0" encoding="UTF-8"?>
<web-app xmnls="http://xmlns.jcp.org/xml/ns/javaee"
        xmnls:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmnls
jcp.org/xml/ns/javaee/web-app_3_1.xsd"
        version="3.1">
         
         <display-name>Mvc Dispatcher</display-name>
         <servlet>
         	<servlet-name>Controller</servlet-name>
         	<servlet-class>web.app.ControlServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>Controller</servlet-name>
         	<url-pattern>*.action</url-pattern>
         </servlet-mapping>
         
</web-app>